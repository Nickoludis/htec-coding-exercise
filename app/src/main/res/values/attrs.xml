<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Custom font textView -->
    <declare-styleable name="CustomFontTextView">
        <attr name="customFontFamily" />
    </declare-styleable>

    <attr name="customFontFamily">
        <enum name="FONT_STYLE_1" value="0" />
        <enum name="FONT_STYLE_2" value="1" />
        <enum name="FONT_STYLE_3" value="2" />
        <enum name="FONT_STYLE_4" value="3" />
        <enum name="FONT_STYLE_5" value="4" />
        <enum name="FONT_STYLE_6" value="5" />
        <enum name="FONT_STYLE_7" value="6" />
        <enum name="FONT_STYLE_8" value="7" />
        <enum name="FONT_STYLE_9" value="8" />
        <enum name="FONT_STYLE_10" value="9" />
        <enum name="FONT_STYLE_11" value="10" />
        <enum name="FONT_STYLE_12" value="11" />
    </attr>

    <declare-styleable name="LotteryButton">
        <attr name="buttonColor" />
    </declare-styleable>

    <attr name="buttonColor">
        <enum name="SELECTOR_1" value="0" />
        <enum name="SELECTOR_2" value="1" />
        <enum name="SELECTOR_3" value="2" />
    </attr>

</resources>